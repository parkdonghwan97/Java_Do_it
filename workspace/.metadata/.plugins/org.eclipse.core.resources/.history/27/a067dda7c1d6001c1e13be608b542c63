package binary;

public class BinaryTEst {

	public static void main(String[] args) {
		
		int num = 10;
		int bNum = 0B1010 ;    // 0B -> 2진수
		int oNum = 012  ;      // 0  -> 8진수
		int hNum = 0XA  ;      // 0X  -> 16진수
		
		

		System.out.println("10진수10 "+num);
		System.out.println("2진수 1010"+bNum);
		System.out.println("8진수 12"+oNum);
		System.out.println("16진수 A"+hNum);
		
	}

}
