package binary;

public class BinaryTEst {

	public static void main(String[] args) {
		
		int num = 10;
		int bNum = 0B1010 ;    // 0B -> 2진수
		int oNum = 012  ;      // 0  -> 8진수
		int hNum = 0XA  ;      // h  -> 16진수
		
		

		System.out.println(num);
		System.out.println(bNum);
		System.out.println(oNum);
		System.out.println(hNum);
		
	}

}
